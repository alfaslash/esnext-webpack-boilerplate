	"devDependencies": {
		"babel-cli": "^6.24.0",
		"babel-loader": "^6.4.1",
		"babel-polyfill": "^6.23.0",
		"babel-preset-latest": "^6.24.0",
		"css-loader": "^0.27.3",
		"express": "^4.15.2",
		"imagemin-webpack-plugin": "^1.4.4",
		"less": "^2.7.2",
